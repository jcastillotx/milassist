# Local Development Environment Variables
# Copy this file to .env and fill in your actual values

# Supabase Database (Local Development)
DATABASE_URI=postgresql://postgres:your-password@db.xxxxxxxxxxxxx.supabase.co:5432/postgres?sslmode=require

# Payload Secret (Local Development)
PAYLOAD_SECRET=local-dev-secret-generate-with-openssl-rand-base64-32

# Server URL (Local)
PAYLOAD_PUBLIC_SERVER_URL=http://localhost:3000

# Node Environment
NODE_ENV=development
